//To create a text database in ROM(VIT UNIVERSITY) and store it in two locations in RAM starting at 40H and 60H
ORG 000H
MOV DPTR,#200H
MOV R0,#40H
MOV R1,#0EH
LOOP: CLR A
MOVC A,@A+DPTR
MOV @R0,A
INC R0
INC DPTR
DJNZ R1,LOOP
MOV R0,#40H
MOV R1,#60H
MOV R3,#0EH
LOOP2:CLR A
MOV A,@R0
MOV @R1,A
INC R0
INC R1
DJNZ R3,LOOP2
HERE: SJMP HERE
ORG 200H
DB 'VIT UNIVERSITY'
END
